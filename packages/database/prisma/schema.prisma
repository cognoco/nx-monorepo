generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model HealthCheck {
  id        String   @id @default(uuid()) @db.Uuid
  message   String
  timestamp DateTime @default(now()) @db.Timestamptz(6)

  @@map("health_checks")
}
